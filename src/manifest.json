{
    "manifest_version": 3,
    "name": "Tab Zen",
    "description": "Clean up your tab chaos with one click. Group, save, and manage tabs easily.",
    "version": "1.0.0",
    "permissions": ["tabs", "storage", "scripting"],
    "action": {
        "default_popup": "src/popup/index.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": ["http://*/*", "https://*/*"]

  }
  